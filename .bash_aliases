alias v="vim"
alias c="clear"
alias q="exit"

alias ls="ls --color=auto"
alias ll="ls -AlF"

alias gits="git status"
alias gita="git add"
alias gitc="git commit"
alias gitco="git checkout"
alias gith="git log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short"

alias ..="cd .."
alias ...="cd ../.."

