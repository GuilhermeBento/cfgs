alias v="vim"
alias c="clear"
alias q="exit"

alias ls="ls --color=auto"
alias ll="ls -AlF"

alias gs="git status"
alias ga="git add"
alias gc="git commit"
alias gco="git checkout"
alias gh="git log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short"

alias ..="cd .."
alias ...="cd ../.."

